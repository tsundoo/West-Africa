{
    "version": 2,
    "root": "./Projet",
    "framework": "create-react-app",
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ],
    "git": {
      "deploymentEnabled": {
        "master": true
      }
    }
  }