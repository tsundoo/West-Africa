{
    "version": 2,
    "builds": [
        {
            "src": "Projet/package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "build",
                "zeroConfig": true
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ],
    "buildCommand": "cd Projet && CI=false npm run build",
    "installCommand": "cd Projet && npm install",
    "outputDirectory": "Projet/build"
}